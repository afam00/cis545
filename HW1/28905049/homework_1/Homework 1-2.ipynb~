    "origin_cities = pd.DataFrame(routes_df.rename(columns={'source_id': 'airport_id'})\\\n",
    "                             ['airport_id'].drop_duplicates())\n",
    "dest_cities = pd.DataFrame(routes_df.rename(columns={'target_id': 'airport_id'})\\\n",
    "                           ['airport_id'].drop_duplicates())\n",
    "airport_cities = airports_df['airport_id']\n",
    "airport_cities.dropna()\n",
