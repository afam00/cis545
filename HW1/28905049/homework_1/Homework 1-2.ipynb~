    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "engine = sqlite3.connect('HW1_DB')\n",
    "\n",
    "# TODO: Re-load dataframes from SQL\n",
    "\n",
    "### BEGIN SOLUTION\n",
    "airlines_df = pd.read_sql_query('select * from \"airlines\"',con=engine)\n",
    "airports_df = pd.read_sql_query('select * from \"airports\"',con=engine)\n",
    "routes_df = pd.read_sql_query('select * from \"routes\"',con=engine)\n",
    "incidents_df = pd.read_sql_query('select * from \"incidents\"',con=engine)\n",
    "# flights = pd.read_sql_query('select * from \"performance\"',con=engine)\n",
    "### END SOLUTION"
   ]
  },
