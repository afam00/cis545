    "                                 [(flights_s_df['ARR_DELAY_NEW'] >= 20) \\\n",
    "                                  | (flights_s_df['CANCELLED'] > 0)]\\\n",
    "                                 .groupby(['airline_iata',\\\n",
    "                                           'target_iata_icao'])\\\n",
    "                                 ['target_id']\\\n",
    "                                 .count())\n",
    "\n",
    "### END SOLUTION\n",
    "\n",
